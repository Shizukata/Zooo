<Window x:Class="SchoolWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchoolWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Height="10" VerticalAlignment="Top" Background="Orange"/>
        <Frame x:Name="MainFrame" VerticalAlignment="Center" Height="410" Width="800" NavigationUIVisibility="Hidden"/>
        <StackPanel Height="10" VerticalAlignment="Bottom" Background="Orange"/>
    </Grid>
</Window>
